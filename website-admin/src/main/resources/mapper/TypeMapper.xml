<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.noobking.personalwebsite.website.admin.mapper.TypeMapper">
    <sql id="typeAllInfo">
        a.id,
        a.name,
        a.status
    </sql>


    <select id="getAllType" resultType="Type">
        SELECT
            <include refid="typeAllInfo"></include>
        FROM
            type AS a
    </select>

</mapper>